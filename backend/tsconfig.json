{
	"compilerOptions": {
		"lib": [
			"ES2020",
			"webworker"
		],
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"removeComments": true,
		"sourceMap": true,
		"target": "ES2020",
		"outDir": "lib",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"allowJs": true,
		"baseUrl": "./src",
		"paths": {
			"utils/*": ["utils/*"]
		}
	},
	"include": [
		"src/**/*.ts",
		"@types/*"
	],
	"exclude": [
		"node_modules/**/*",
		".webpack/**/*",
		"_warmup/**/*",
		".vscode/**/*"
	]
}
